{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "system",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "removeComments": false,
    "outDir": "./wwwroot/appScripts",
    "sourceMap": true,
    "target": "es6",
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "typings"
  ]
}